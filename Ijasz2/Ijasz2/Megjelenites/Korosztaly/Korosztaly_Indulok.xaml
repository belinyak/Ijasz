<Window x:Class="Ijasz2.Megjelenites.Korosztaly.Korosztaly_Indulok"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Indulók" Height="300" Width="365" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
	<Grid>
		<DataGrid>
			<DataGrid.Columns>
				<DataGridTextColumn Width="150" Binding="{x:Null}" CanUserSort="False" CanUserReorder="False" CanUserResize="False"
				                    ClipboardContentBinding="{x:Null}" Header="Név" />
				<DataGridTextColumn Width="50" Binding="{x:Null}" CanUserSort="False" CanUserReorder="False" CanUserResize="False"
				                    ClipboardContentBinding="{x:Null}" Header="Nem" />
				<DataGridTextColumn Width="100" Binding="{x:Null}" CanUserSort="False" CanUserReorder="False" CanUserResize="False"
				                    ClipboardContentBinding="{x:Null}" Header="Korosztály" />
				<DataGridTextColumn Width="48" Binding="{x:Null}" CanUserSort="False" CanUserReorder="False" CanUserResize="False"
				                    ClipboardContentBinding="{x:Null}" Header="Életkor" />
			</DataGrid.Columns>
		</DataGrid>
	</Grid>
</Window>