<Window x:Class="Ijasz2.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Loaded="MainWindow_OnLoaded"
        Title="Íjász program" Height="768" Width="1024" ResizeMode="NoResize" WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="710" />
            <RowDefinition Height="Auto" />
        </Grid.RowDefinitions>
        <Label Content="Turul Koppány Íjászai Kft." HorizontalAlignment="Stretch" Grid.Row="1" />
        <TabControl Grid.Row="0">
            <TabItem Header="Versenysorozatok">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="300" />
                        <ColumnDefinition Width="462" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <DataGrid IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled"
					          x:Name="VersenysorozatGrid" AutoGenerateColumns="False" HorizontalAlignment="Stretch"
                              >
                        <!--azért, hogy ne legyen selected cell-->
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Versenysorozat_Modositas" />
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Azonosito}" CanUserSort="False"
							                    CanUserReorder="False" CanUserResize="False" Width="100" Header="Azonosító" />
                            <DataGridTextColumn Binding="{Binding Megnevezes}" CanUserSort="False"
							                    CanUserReorder="False" CanUserResize="False" Width="100" Header="Megnevezés" />
                            <DataGridTextColumn Binding="{Binding VersenyekSzama}" CanUserSort="False"
							                    CanUserReorder="False" CanUserResize="False" Width="100" Header="Versenyek száma" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button Content="Törlés" Grid.Column="2" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16"
					        Click="BtnVersenysorozatTorles_OnClick" />
                    <Button Content="Hozzáadás" Grid.Column="3" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16"
					        Click="BtnVersenysorozatHozzaadas_OnClick" />
                </Grid>
            </TabItem>
            <TabItem Header="Versenyek">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="705" />
                        <ColumnDefinition Width="57" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <DataGrid
						IsReadOnly="True"
						SelectionMode="Single"
						SelectionUnit="FullRow"
						CanUserResizeRows="False"
						RowHeaderWidth="0"
						HorizontalScrollBarVisibility="Disabled"
						x:Name="VersenyGrid" AutoGenerateColumns="False" HorizontalAlignment="Stretch">

                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Verseny_Modositas" />
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Azonosito, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Azonosító" />
                            <DataGridTextColumn Binding="{Binding Megnevezes, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="145"
							                    Header="Megnevezés" />
                            <DataGridTextColumn Binding="{Binding Datum, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="70" Header="Dátum" />
                            <DataGridTextColumn Binding="{Binding Versenysorozat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Versenysorozat" />
                            <DataGridTextColumn Binding="{Binding Osszes, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="55" Header="Lövések" />
                            <DataGridTextColumn Binding="{Binding Allomasok, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="65"
							                    Header="Állomások" />
                            <DataGridTextColumn Binding="{Binding Indulok, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="50" Header="Indulók" />
                            <DataGridCheckBoxColumn Binding="{Binding Lezarva, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Header="Lezárva" Width="50" />
                            <DataGridCheckBoxColumn Binding="{Binding DuplaBeirolap, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Header="D. Beírólap" Width="70" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button Content="Törlés" Grid.Column="2" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16" Click="BtnVersenyTorles_OnClick" />
                    <Button Content="Hozzáadás" Grid.Column="3" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnVersenyHozzaadas_Click" />
                </Grid>
            </TabItem>
            <TabItem Header="Korosztályok">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="565" />
                        <ColumnDefinition Width="197" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="630" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <DataGrid x:Name="KorosztalyGrid" HorizontalAlignment="Stretch" Grid.RowSpan="2"
					          IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          AutoGenerateColumns="False"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled">
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Korosztaly_Modositas" />
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Verseny, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="70" Header="Verseny"
							                    Visibility="Hidden" />
                            <DataGridTextColumn Binding="{Binding Azonosito, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="70"
							                    Header="Azonosító" />
                            <DataGridTextColumn Binding="{Binding Megnevezes, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Megnevezés" />
                            <DataGridTextColumn Binding="{Binding AlsoHatar, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="70"
							                    Header="Alsó határ" />
                            <DataGridTextColumn Binding="{Binding FelsoHatar, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="70"
							                    Header="Felső határ" />
                            <DataGridCheckBoxColumn Binding="{Binding Nokre, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Width="55" Header="Nőkre" />
                            <DataGridCheckBoxColumn Binding="{Binding Ferfiakra, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Width="55" Header="Férfiakra" />
                            <DataGridTextColumn Binding="{Binding InduloNok, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="45" Header="Nők" />
                            <DataGridTextColumn Binding="{Binding InduloFerfiak, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="45" Header="Férfiak" />
                            <DataGridCheckBoxColumn Binding="{Binding Egyben, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Width="55" Header="Egyben" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Label Content="Verseny azonosító:" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Center"
					       VerticalAlignment="Top" Margin="12,5,11,0" />
                    <ComboBox x:Name="cboVerseny" DisplayMemberPath="Azonosito" Grid.Row="0" Grid.Column="3"
					          HorizontalAlignment="Left" Width="100" VerticalAlignment="Top" Margin="0,10,0,0"
					          SelectionChanged="cboVerseny_SelectionChanged" />
                    <Button Content="Számolás" Grid.Column="2" Grid.Row="0"
					        HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16"
					        Click="btnKorosztalySzamolas_Click" />
                    <Button Content="Indulók" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnKorosztalyIndulok_Click" />
                    <Button Content="Törlés" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16" Click="btnKorosztalyTorles_Click" />
                    <Button Content="Hozzáadás" Grid.Column="3" Grid.Row="1"
					        HorizontalAlignment="Left" VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16"
					        Click="btnKorosztalyHozzaadas_Click" />
                </Grid>
            </TabItem>
            <TabItem Header="Íjtípusok">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="300" />
                        <ColumnDefinition Width="462" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="IjtipusGrid"
					          HorizontalAlignment="Stretch"
					          IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          AutoGenerateColumns="False"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled">
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Ijtipus_Modositas" />
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Azonosito, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Azonosító" />
                            <DataGridTextColumn Binding="{Binding Megnevezes, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="145"
							                    Header="Megnevezés" />
                            <DataGridTextColumn Binding="{Binding Sorszam, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="55" Header="Sorszám" />
                            <DataGridTextColumn Binding="{Binding Eredmenyek, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Eredmenyek" Visibility="Hidden" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button Content="Törlés" Grid.Column="2" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16" Click="BtnIjtipusTorles_OnClick" />
                    <Button Content="Hozzáadás" Grid.Column="3" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnIjtipusHozzaadas_Click" />
                </Grid>
            </TabItem>
            <TabItem Header="Egyesületek">
                <Grid Background="#FFE5E5E5">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="670" />
                        <ColumnDefinition Width="92" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="630" />
                        <RowDefinition></RowDefinition>
                    </Grid.RowDefinitions>
                    <DataGrid x:Name="EgyesuletGrid"
                              Grid.RowSpan="2"
					          HorizontalAlignment="Stretch"
					          IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          AutoGenerateColumns="False"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled">
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Egyesulet_Modositas" />
                            </Style>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Azonosito, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="200" Header="Név" />
                            <DataGridTextColumn Binding="{Binding Cim, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="200" Header="Cím" />
                            <DataGridTextColumn Binding="{Binding Vezeto, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="150" Header="Vezető" />
                            <DataGridTextColumn Binding="{Binding Telefon1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Telefon" Visibility="Hidden" />
                            <DataGridTextColumn Binding="{Binding Telefon2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Telefon2" Visibility="Hidden" />
                            <DataGridTextColumn Binding="{Binding Email1, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100" Header="E-mail"
							                    Visibility="Hidden" />
                            <DataGridTextColumn Binding="{Binding Email2, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="E-mail2" Visibility="Hidden" />
                            <DataGridCheckBoxColumn Binding="{Binding Listazando, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False" Header="Listázandó"
							                        Width="70" />
                            <DataGridTextColumn Binding="{Binding TagokSzama, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="50" Header="Tagok" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button Content="Törlés" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16" Click="btnEgyesuletTorles_Click" />
                    <Button Content="Hozzáadás" Grid.Column="3" Grid.Row="1"  HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnEgyesuletHozzaadas_Click" />
                    <Button Content="Tagok" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnTagok_Click" />
                </Grid>
            </TabItem>
            <TabItem Header="Indulók">
                <Grid Background="#FFE5E5E5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="630" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <!--columnwidth-->
                        <ColumnDefinition Width="590" />
                        <!--762-columnwidth-->
                        <ColumnDefinition Width="172" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="InduloGrid"
					          Grid.RowSpan="2"
					          HorizontalAlignment="Stretch"
					          IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          AutoGenerateColumns="False"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled"
                              >
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Indulo_Modositas" />
                            </Style>
                            <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="#3399FF"/>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Nev, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="150" Header="Név"/>
                            <DataGridTextColumn Binding="{Binding Nem, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="40" Header="Nem" />
                            <DataGridTextColumn Binding="{Binding SzuletesiDatum, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Születési dátum" />
                            <DataGridTextColumn Binding="{Binding Engedely, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Engedély" />
                            <DataGridTextColumn Binding="{Binding Egyesulet, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Egyesület" />
                            <DataGridTextColumn Binding="{Binding Eredmenyek, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Eredmények" />
                        </DataGrid.Columns>
                    </DataGrid>
                    <Button Content="Törlés" Grid.Column="2" Grid.Row="1" HorizontalAlignment="Center"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="18,0,17,16" Click="btnInduloTorles_Click" />
                    <Button Content="Hozzáadás" Grid.Column="3" Grid.Row="1" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnInduloHozzaadas_Click" />
                    <Button Content="Beírás" Grid.Column="3" Grid.Row="0" HorizontalAlignment="Left"
					        VerticalAlignment="Bottom" Width="96" Height="32" Margin="0,0,0,16" Click="btnInduloBeiras_Click" />
                    <Label Content="Induló neve:" Grid.Row="0" Grid.Column="2" HorizontalAlignment="Right"
					       VerticalAlignment="Top" Margin="12,5,11,0" />
                    <TextBox x:Name="txtInduloNev" Grid.Row="0" Grid.Column="3" HorizontalAlignment="Left" Height="23"
					         TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Margin="0,8,0,0" TextChanged="TxtInduloNev_OnTextChanged"/>


                </Grid>
            </TabItem>
            <TabItem Header="Eredmények">
                <Grid Background="#FFE5E5E5">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="40" />
                        <RowDefinition Height="40" />
                        <RowDefinition Height="550" />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <!--columnwidth-->
                        <ColumnDefinition Width="735" />
                        <!--762-columnwidth-->
                        <ColumnDefinition Width="27" />
                        <ColumnDefinition Width="131" />
                        <ColumnDefinition Width="131" />
                    </Grid.ColumnDefinitions>
                    <DataGrid x:Name="EredmenyGrid"
					          Grid.RowSpan="4"
					          HorizontalAlignment="Stretch"
					          IsReadOnly="True"
					          SelectionMode="Single"
					          SelectionUnit="FullRow"
					          AutoGenerateColumns="False"
					          CanUserResizeRows="False"
					          RowHeaderWidth="0"
					          HorizontalScrollBarVisibility="Disabled"
                              >
                        <DataGrid.Resources>
                            <Style TargetType="DataGridCell">
                                <Setter Property="BorderThickness" Value="0" />
                                <Setter Property="FocusVisualStyle" Value="{x:Null}" />
                            </Style>
                            <Style TargetType="DataGridRow">
                                <EventSetter Event="MouseDoubleClick" Handler="Eredmeny_Modositas" />
                            </Style>
                            <SolidColorBrush x:Key="{x:Static SystemColors.InactiveSelectionHighlightBrushKey}" Color="#3399FF"/>
                        </DataGrid.Resources>
                        <DataGrid.Columns>
                            <DataGridTextColumn Binding="{Binding Verseny, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Verseny" Visibility="Hidden" />
                            <DataGridTextColumn Binding="{Binding Indulo, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="200" Header="Induló" />
                            <DataGridTextColumn Binding="{Binding Sorszam, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="60" Header="Sorszám" />
                            <DataGridTextColumn Binding="{Binding Ijtipus, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="100"
							                    Header="Íjtípus" />
                            <DataGridTextColumn Binding="{Binding Csapat, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="50" Header="Csapat" />
                            <DataGridTextColumn Binding="{Binding Talalat10, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="25" Header="10" />
                            <DataGridTextColumn Binding="{Binding Talalat8, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="25" Header="8" />
                            <DataGridTextColumn Binding="{Binding Talalat5, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="25" Header="5" />
                            <DataGridTextColumn Binding="{Binding Melle, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="40" Header="Mellé" />
                            <DataGridTextColumn Binding="{Binding OsszPont, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="50" Header="Összes" />
                            <DataGridTextColumn Binding="{Binding Szazalek, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                    CanUserSort="False" CanUserReorder="False" CanUserResize="False" Width="25" Header="%" />
                            <DataGridCheckBoxColumn Binding="{Binding Megjelent, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"
							                        CanUserSort="False" CanUserReorder="False" CanUserResize="False"
							                        ClipboardContentBinding="{x:Null}" Header="Megjelent" Width="85" />
                            <DataGridCheckBoxColumn
								Binding="{Binding KorosztalyModositott, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}" CanUserSort="False"
								CanUserReorder="False" CanUserResize="False" ClipboardContentBinding="{x:Null}" Header="Korosztaly módosított"
